; model

enter   -> s0 , enter , .S :: ctl-true .
s0      -> s1 , s2 , .S    :: a1 and a2 and a3 and a4 .
s1      -> s3 , .S         :: a1 and a4 .
s2      -> s4 , .S         :: a1 and a2 a3 .
s3      -> s4 , .S         :: a1 and a3 and a4 .
s4      -> s5 , .S         :: a1 and a2 .
s5      -> exit , .S       :: a1 and a4 .
exit    -> exit , .S       :: ctl-false .

; end model

; satis

s4 |= A next s5
